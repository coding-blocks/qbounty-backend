{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "indent": [
      "spaces"
    ],
    "quotemark": [
      "single"
    ],
    "semicolon": false,
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "interface-name": false,
    "max-classes-per-file": [false]
  },
  "rulesDirectory": []
}
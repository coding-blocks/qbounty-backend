{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "indent": [
      "spaces"
    ],
    "quotemark": [
      "single"
    ],
    "semicolon": false,
    "object-literal-sort-keys": false,
    "no-var-requires": false,
    "trailing-comma": false
  },
  "rulesDirectory": []
}